	<queryString>
		<![CDATA[SELECT a.vendcode,a.vendname,a.active, a.taxcode,a.vendortxt,
       CASE WHEN a.active=1
            THEN CONCAT('Vendor Code : ',a.vendcode,' ',a.vendname,' - Active ' )
            ELSE CONCAT('Vendor Code : ',a.vendcode,' ',a.vendname,' - Not Active  ') END STATUS,
       CASE WHEN a.active=1 THEN 'Active' ELSE 'Not Active' END daktive,
       a.grpcode,a.dscriptiongrp,a.bankcode,
       a.curnccode,a.cmpnyid,a.notes,
       MAX(a.AKantor)AKantor,MAX(a.AKirim)AKirim,MAX(a.ATagih)Atagih,
       MAX(a.LandLine)LandLine,MAX(a.Mobile)Mobile,MAX(a.Fax)Fax,
       MAX(a.Email)Email,MAX(a.Web)Web,
       a.distcode,a.descdistcode,a.accfmttd,
       IFNULL(b.yop,0)yop,IFNULL(b.mop,0)mop,
       IFNULL(b.invamt,0)invamt,IFNULL(b.invcnt,0)invcnt,
       IFNULL(b.payamt,0)payamt,IFNULL(b.paycnt,0)paycnt,
       IFNULL(b.discamt,0)discamt,IFNULL(b.disccnt,0)disccnt,
       IFNULL(b.discamt,0)discamt,IFNULL(b.disccnt,0)disccnt,
       IFNULL(b.dnamt,0)dnamt,IFNULL(b.dncnt,0)dncnt,
       IFNULL(b.cnamt,0)cnamt,IFNULL(b.cncnt,0)cncnt,
       IFNULL(b.adjamt,0)adjamt,IFNULL(b.adjcnt,0)adjcnt,
       IFNULL(b.wroffamt,0)wroffamt,IFNULL(b.wroffcnt,0)wroffcnt,
       IFNULL(b.inttamt,0)inttamt,IFNULL(b.inttcnt,0)inttcnt,
       IFNULL(b.rtrchckamt,0)rtrchckamt,IFNULL(b.rtrchckcnt,0)rtrchckcnt,
       IFNULL(b.invpaidamt,0)invpaidamt,IFNULL(b.invpaidcnt,0)invpaidcnt,
       IFNULL(c.yop,0)gyop,IFNULL(c.mop,0)gmop,
       IFNULL(c.invamt,0)ginvamt,IFNULL(c.invcnt,0)ginvcnt,
       IFNULL(c.payamt,0)gpayamt,IFNULL(c.paycnt,0)gpaycnt,
       IFNULL(c.discamt,0)gdiscamt,IFNULL(c.disccnt,0)gdisccnt,
       IFNULL(c.discamt,0)gdiscamt,IFNULL(c.disccnt,0)gdisccnt,
       IFNULL(c.dnamt,0)gdnamt,IFNULL(c.dncnt,0)gdncnt,
       IFNULL(c.cnamt,0)gcnamt,IFNULL(c.cncnt,0)gcncnt,
       IFNULL(c.adjamt,0)gadjamt,IFNULL(c.adjcnt,0)gadjcnt,
       IFNULL(c.wroffamt,0)gwroffamt,IFNULL(c.wroffcnt,0)gwroffcnt,
       IFNULL(c.inttamt,0)ginttamt,IFNULL(c.inttcnt,0)ginttcnt,
       IFNULL(c.rtrchckamt,0)grtrchckamt,IFNULL(c.rtrchckcnt,0)grtrchckcnt,
       IFNULL(c.invpaidamt,0)ginvpaidamt,IFNULL(c.invpaidcnt,0)ginvpaidant,
       now() tanggalan
FROM
(
SELECT a.vendcode,a.vendname,a.notes,a.active,a.bankcode,a.curnccode,
       a.termcode,a.taxcode,
       a.cmpnyid,a.grpcode,h.dscription dscriptiongrp,
       concat(
        if($P{vendcodefr}='','ALL VENDOR',
                              CONCAT($P{vendcodefr},' - ',(select vendname from ap_vendor
                                                           WHERE vendcode=$P{vendcodefr}
                                                           order by vendcode limit 1)
                                    )
          ),
        ' TO ',
        if($P{vendcodeto}='','ALL VENDOR',
                              CONCAT($P{vendcodeto},' - ',(select vendname from ap_vendor
                                                           WHERE vendcode=$P{vendcodeto}
                                                           order by vendcode limit 1)
                                    )
          )
       )vendortxt,
       CASE WHEN c.addrtypecode='01' THEN CONCAT(c.address,'',c.city,'',c.state,'',c.zipcode) ELSE '' END AKantor,
       CASE WHEN c.addrtypecode='02' THEN CONCAT(c.address,'',c.city,'',c.state,'',c.zipcode) ELSE '' END AKirim,
       CASE WHEN c.addrtypecode='03' THEN CONCAT(c.address,'',c.city,'',c.state,'',c.zipcode) ELSE '' END ATagih,
       CASE WHEN e.phonetypecode='01' THEN e.phone ELSE '' END LandLine,
       CASE WHEN e.phonetypecode='02' THEN e.phone ELSE '' END Mobile,
       CASE WHEN e.phonetypecode='03' THEN e.phone ELSE '' END Fax,
       CASE WHEN e.phonetypecode='04' THEN e.phone ELSE '' END Email,
       CASE WHEN e.phonetypecode='05' THEN e.phone ELSE '' END Web,
       a.distcode,i.dscription descdistcode,a.glacccode,b.accfmttd
FROM ap_vendor a
LEFT JOIN gl_account b
     ON b.acccode=a.glacccode
LEFT JOIN op_address c
     ON c.ownercode=a.vendcode
LEFT JOIN op_addrtype d
     ON d.addrtypecode=c.addrtypecode
LEFT JOIN op_phone e
     ON e.ownercode=c.ownercode AND e.addrcode=c.addrcode
        AND e.addrtypecode=d.addrtypecode
LEFT JOIN op_phonetype f
     ON f.phonetypecode=e.phonetypecode
LEFT JOIN ap_vendgrp h
     ON h.grpcode=a.grpcode
LEFT JOIN ap_distcode i
     ON i.distcode=a.distcode
WHERE
      (UCASE(a.vendcode) BETWEEN
          if($P{vendcodefr}='',
             (select vendcode from ap_vendor order by vendcode asc limit 1),$P{vendcodefr}
             )
                         and
          if($P{vendcodeto}='',
             (select vendcode from ap_vendor order by vendcode desc limit 1),$P{vendcodeto}
             )
      )
      AND a.active=$P{active}
      AND (ucase(a.grpcode)>='' AND ucase(a.grpcode)<='zzzzz')
ORDER BY a.vendcode,a.active,a.grpcode,a.distcode
)a
LEFT JOIN ap_vendorsts b
     ON b.vendcode=a.vendcode
LEFT JOIN ap_vendgrpsts c
     ON c.grpcode=a.grpcode
where b.yop between cast($P{yopfr} as decimal) and cast($P{yopto} as decimal)
GROUP BY a.vendcode,a.active,a.grpcode,a.distcode
ORDER BY a.vendcode,a.active,a.grpcode,a.distcode]]>
