	<queryString>
		<![CDATA[SELECT MAX(a.custmrcode) custmrcode, MAX(a.custmrname)custmrname,
           a.offadd,a.delivadd,a.billadd,a.homephone,a.handphone,a.faxno,a.email,a.web,
           a.STATUS,a.active,a.daktive,
           a.accsetcode,a.daccset,a.cyclcode,a.dcycle,a.intrschrcode,a.dinterest,
           a.termcode,a.dterm,
           a.taxcode,a.dtax,a.credlimitamt,
           a.ordersbalamt,a.yop,a.mop,
           a.holdtxt, MAX(a.invamt)invamt,MAX(a.invcnt)invcnt,MAX(a.payamt)payamt,
           MAX(a.paycnt)paycnt,MAX(a.cnamt)cnamt,
           MAX(a.cncnt)cncnt,MAX(a.dnamt)dnamt,
           MAX(a.dncnt)dncnt,MAX(a.discamt)discamt,MAX(a.disccnt)disccnt,
           MAX(a.adjamt)adjamt,MAX(a.adjcnt)adjcnt,MAX(a.wroffamt)wroffamt,MAX(a.wroffcnt)wroffcnt,
           MAX(a.inttamt)inttamt,MAX(a.inttcnt)inttcnt,MAX(a.rtrchckamt)rtrchckamt,
           MAX(a.rtrchckcnt)rtrchckcnt,MAX(a.invpaidamt)invpaidamt,MAX(a.invpaidcnt)invpaidcnt,
           a. txtcustcode,
           a.txtmonyon,
           a.cmpnyid,a.tanggalan,jlaptxt
FROM
(
SELECT  a.custmrcode, a.custmrname,
            CASE WHEN a.custmrcode AND h.addrtypecode='01' THEN CONCAT(h.address,' ',h.city,' ',h.state,' ',h.zipcode) ELSE '' END offadd,
           CASE WHEN a.custmrcode AND h.addrtypecode='02' THEN CONCAT(h.address,' ',h.city,' ',h.state,' ',h.zipcode) ELSE '' END delivadd,
           CASE WHEN a.custmrcode AND h.addrtypecode='03' THEN CONCAT(h.address,' ',h.city,' ',h.state,' ',h.zipcode) ELSE '' END billadd,
            CASE WHEN a.custmrcode AND i.phonetypecode='01' THEN phone ELSE '' END homephone,
           CASE WHEN a.custmrcode AND i.phonetypecode='02' THEN phone ELSE '' END handphone,
           CASE WHEN a.custmrcode AND i.phonetypecode='03' THEN phone ELSE '' END faxno,
           CASE WHEN a.custmrcode AND i.phonetypecode='04' THEN phone ELSE '' END email,
           CASE WHEN a.custmrcode AND i.phonetypecode='05' THEN phone ELSE '' END web,
           CASE WHEN a.active=1 THEN CONCAT('Custommer Code : ',a.custmrcode,' ',a.custmrname,' - Active ' ) ELSE CONCAT('Custommer Code : ',a.custmrcode,' ', a.custmrname,' - Not Active ') END STATUS,
           a.active,CASE WHEN a.active=1 THEN 'Active' ELSE 'Not Active' END daktive,
           a.accsetcode,c.dscription daccset,
           a.cyclcode,d.dscrition dcycle,a.intrschrcode,e.dscription dinterest,a.termcode,f.dscription dterm,
           a.taxcode,g.dscription dtax,ifnull(a.credlimitamt,0)credlimitamt,ifnull(a.ordersbalamt,0)ordersbalamt,
           b.yop,b.mop,
           cASE WHEN a.hold=0 then 'Not Hold' else 'Hold' end holdtxt,
           IFNULL(b.invamt,0)invamt,IFNULL(b.invcnt,0)invcnt,IFNULL(b.payamt,0)payamt,
           IFNULL(b.paycnt,0)paycnt,IFNULL(b.cnamt,0)cnamt,
           IFNULL(b.cncnt,0)cncnt,IFNULL(b.dnamt,0)dnamt,
           IFNULL(b.dncnt,0)dncnt,IFNULL(b.discamt,0)discamt,IFNULL(b.disccnt,0)disccnt,
           IFNULL(b.adjamt,0)adjamt,IFNULL(b.adjcnt,0)adjcnt,IFNULL(b.wroffamt,0)wroffamt,IFNULL(b.wroffcnt,0)wroffcnt,
           IFNULL(b.inttamt,0)inttamt,IFNULL(b.inttcnt,0)inttcnt,IFNULL(b.rtrchckamt,0)rtrchckamt,IFNULL(b.rtrchckcnt,0)rtrchckcnt,IFNULL(b.invpaidamt,0)invpaidamt,IFNULL(b.invpaidcnt,0)invpaidcnt,
           IF($P{custcodefr}='' AND $P{custcodeto}='','ALL CUSTOMMER CODE',
              CONCAT(IF($P{custcodefr}='' ,(SELECT CONCAT(custmrcode,'-',custmrname)  FROM ar_customer ORDER BY custmrcode ASC LIMIT 1),
                                                       IFNULL((SELECT CONCAT(custmrcode,'-',custmrname)  FROM ar_customer WHERE custmrcode= $P{custcodefr} LIMIT 1),
                                                                   $P{custcodefr})
                            ),
                          '  S/D  ',
                   	  IF($P{custcodeto}='' ,(SELECT CONCAT(custmrcode,'-',custmrname)  FROM ar_customer ORDER BY custmrcode DESC LIMIT 1),
                                                      IFNULL((SELECT CONCAT(custmrcode,'-',custmrname)  FROM ar_customer WHERE custmrcode= $P{custcodeto} LIMIT 1),
                                                                  $P{custcodeto})
                            )
                         )
            ) txtcustcode,
           concat('Periode :  ',CAST($P{mon} AS CHARACTER ),'/',$P{yon}) txtmonyon,
           a.cmpnyid,NOW() tanggalan,
           CASE WHEN $P{jenislaporan}='0' THEN 'ADDRESS' ELSE 'FORM' END jlaptxt
FROM ar_customer a
LEFT JOIN ar_customersts b
     ON b.custmrcode=a.custmrcode
        AND b.yop = CAST($P{yon} AS DECIMAL)
        AND b.mop = $P{mon}
LEFT JOIN ar_accset c
      ON c.accsetcode=a.accsetcode
LEFT JOIN ar_billcycl d
      ON d.cyclcode=a.cyclcode
LEFT JOIN ar_interest e
      ON e.intrschrcode=a.intrschrcode
LEFT JOIN op_paymentterm f
      ON f.termcode=a.termcode
LEFT JOIN op_tax g
      ON g.taxcode=a.taxcode
LEFT JOIN op_address h
      ON h.ownercode=a.custmrcode
LEFT JOIN op_phone i
      ON h.ownercode=a.custmrcode
WHERE a.active=$P{aktif} AND
           (a.custmrcode BETWEEN IF($P{custcodefr}='' ,(SELECT custmrcode FROM ar_customer ORDER BY custmrcode ASC LIMIT 1),
                                                                                    $P{custcodefr}
                                                )
                                       AND IF($P{custcodeto}='' ,(SELECT custmrcode FROM ar_customer ORDER BY custmrcode DESC LIMIT 1),
                                                                                    $P{custcodeto}
                                                )
           )
ORDER BY a.custmrcode,b.yop,b.mop
)a
group by a.custmrcode,a.yop,a.mop]]>
