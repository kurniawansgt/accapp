	<queryString>
		<![CDATA[SELECT  a.cmpnyid,a.batchno, a.batchdesc,
	a.batchdate,a.rcpentrcnt,a.totrcpamt,
	CASE WHEN a.batchtype=1 THEN "Entried"
               WHEN a.batchtype=2 THEN "Imported"
               WHEN a.batchtype=3 THEN "Generated"
	END batchtype,a.bankcode,i.bankname,a.curnccode currency,
	CASE WHEN $P{status1}='1' AND UCASE($P{status2})='R' AND UCASE($P{status4})='D' THEN "Open"
               WHEN UCASE($P{status1})='O' AND $P{status2}='2' AND UCASE($P{status4})='D' THEN "Posted"
               WHEN UCASE($P{status1})='O' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='3' AND $P{status4}='D' THEN "Error"
               WHEN UCASE($P{status1})='O' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='E' AND $P{status4}='4' THEN "Deleted"
               WHEN $P{status1}='1' AND $P{status2}='2'
                    AND UCASE($P{status3})='E' AND UCASE($P{status4})='D' THEN "Open,Posted"
               WHEN $P{status1}='1' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='3' AND UCASE($P{status4})='D' THEN "Open,Error"
               WHEN $P{status1}='1' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='E' AND UCASE($P{status4})='4' THEN "Open,Deleted"
               WHEN UCASE($P{status1})='O' AND UCASE($P{status2})='2'
                    AND $P{status3}='3' AND UCASE($P{status4})='D' THEN "Posted,Error"
               WHEN ucase($P{status1})='O' AND UCASE($P{status2})='2'
                    AND UCASE($P{status3})='E' AND UCASE($P{status4})='4' THEN "Posted,Deleted"
               WHEN ucase($P{status1})='O' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='3' AND UCASE($P{status4})='4' THEN "Eror,Deleted"
               ELSE "Open, Posted, Error, Deleted"
          END ctksts,
	CASE WHEN a.batchsts='1' THEN "Open"
               WHEN a.batchsts='2' THEN "Posted"
               WHEN a.batchsts='3' THEN "Error"
               WHEN a.batchsts='4' THEN "Deleted"
               ELSE "Open, Posted, Error, Deleted"
          END batchsts,
         CASE WHEN a.readytpost='0' THEN "Open"
              WHEN a.readytpost='1' THEN "ReadyToPost"
              ELSE "Open & ReadyToPost"
         END batchrtp,
         b.docentry,b.docnum,b.dscription descpayh,
         case when b.doctype='1' then 'Receipt'
              when b.doctype='2' then 'Pre Payment'
              when b.doctype='3' then 'Unapplied Cash'
              when b.doctype='4' then 'Apply Document'
              when b.doctype='5' then 'Misc. Receipt'
         end transtype,
         b.custmrcode,f.custmrname,
         b.docdate,b.mop,b.yop,b.docnum nocheck,
         case when b.paycode='0' then 'Cash' else 'Chek' end receiptype,
         b.payor payer,
         IFNULL(b.doctotalamt,0) ammount,IFNULL(b.unapltotalamtc,0)unapltotalamt,b.curnccode,
         c.glreff,IFNULL(d.accfmttd,'') glaccount,
         CASE WHEN c.doctype=4 THEN "IN"
	    WHEN c.doctype=6 THEN "DN"
	    WHEN c.doctype=5 THEN "CN"
	    WHEN c.doctype=7 THEN "IT"
         END doctype,c.docnum cdocnum,g.doctotalamtr pendblnc,c.payamt applmamt,(g.doctotalamtr+c.payamt)netblnc,
         g.doctotalamtr curramt,h.doctotalamt oriamt,g.doctotalamtr pendingamt,                                                                         h.custmrpono pono,h.orderno noorder,h.docdate docdatedetail,h.docduedate duedatedetail,
         CASE WHEN b.doctype='4' THEN SUM(IFNULL(c.payamt,0))  ELSE 0  END ttlinv,
         CASE WHEN b.doctype='5' THEN SUM(IFNULL(c.payamt,0))  ELSE 0  END ttldn,
         CASE WHEN b.doctype='6' THEN SUM(IFNULL(c.payamt,0))  ELSE 0  END ttlcn,
         CASE WHEN b.doctype='7' THEN SUM(IFNULL(c.payamt,0))  ELSE 0  END ttlint,
         CONCAT( if(b.batchno<10, concat('0',cast(b.batchno AS CHARACTER)), cast(b.batchno AS CHARACTER)),'-',
                 if(b.docentry<10, concat('0',cast(b.docentry AS CHARACTER)), cast(b.docentry AS CHARACTER)),'-'
               )begrp
FROM ar_rcpbatchlist a
	INNER JOIN ar_rcph b
		ON a.batchno=b.batchno
		AND (a.batchno BETWEEN $P{batchnofr} AND $P{batchnoto})
		AND (LEFT(a.batchdate,10) BETWEEN $P{batchdatefro} AND $P{batchdatetoo})
		AND (a.batchtype =$P{entried} OR a.batchtype=$P{imported} OR a.batchtype=$P{transfer})
		AND (a.readytpost=0 OR a.readytpost=1)
		and (a.batchsts=$P{status1} or a.batchsts=$P{status2} or a.batchsts=$P{status3} or a.batchsts=$P{status4})
		AND (a.swprinted=$P{swprint}  OR a.swprinted='1' OR a.swprinted='0')
	INNER JOIN ar_rcpd c
		ON b.batchno=c.batchno
		AND b.docentry=c.docentry
	LEFT JOIN gl_account d
		ON  d.acccode=c.glreff
		AND d.cmpnyid=a.cmpnyid
	LEFT JOIN ar_optional e
		ON e.cmpnyid=a.cmpnyid
          	LEFT JOIN ar_customer f
		ON f.custmrcode=b.custmrcode
          	LEFT JOIN ar_invobl g
		ON g.docnum=c.docnum AND g.doctype=c.doctype AND g.custmrcode=c.custmrcode
          	LEFT JOIN ar_invh h
		ON h.docnum=c.docnum AND h.doctype=c.doctype AND g.custmrcode=c.custmrcode
          	LEFT JOIN cb_bank i
		ON i.bankcode=a.bankcode
GROUP BY a.batchno,b.docentry,c.linenum]]>
	</queryString>
