	<queryString>
		<![CDATA[select e.cmpnyid,
	a.batchno, a.batchdesc,
	a.batchdate,a.inventrcnt,a.totinvamt,
	case when a.batchtype=1 then "Entried"
               when a.batchtype=2 then "Imported"
               when a.batchtype=3 then "Generated"
	end batchtype,
	CASE WHEN $P{status1}='1' AND UCASE($P{status2})='R' AND UCASE($P{status4})='D' THEN "Open"
               WHEN UCASE($P{status1})='O' AND $P{status2}='2' AND UCASE($P{status4})='D' THEN "Posted"
               WHEN UCASE($P{status1})='O' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='3' AND $P{status4}='D' THEN "Error"
               WHEN UCASE($P{status1})='O' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='E' AND $P{status4}='4' THEN "Deleted"
               WHEN $P{status1}='1' AND $P{status2}='2'
                    AND UCASE($P{status3})='E' AND UCASE($P{status4})='D' THEN "Open,Posted"
               WHEN $P{status1}='1' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='3' AND UCASE($P{status4})='D' THEN "Open,Error"
               WHEN $P{status1}='1' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='E' AND UCASE($P{status4})='4' THEN "Open,Deleted"
               WHEN UCASE($P{status1})='O' AND UCASE($P{status2})='2'
                    AND $P{status3}='3' AND UCASE($P{status4})='D' THEN "Posted,Error"
               WHEN ucase($P{status1})='O' AND UCASE($P{status2})='2'
                    AND UCASE($P{status3})='E' AND UCASE($P{status4})='4' THEN "Posted,Deleted"
               WHEN ucase($P{status1})='O' AND UCASE($P{status2})='R'
                    AND UCASE($P{status3})='3' AND UCASE($P{status4})='4' THEN "Eror,Deleted"
               ELSE "Open, Psted, Error, Deleted"
          END ctksts,
	CASE WHEN a.batchsts='1' THEN "Open"
               WHEN a.batchsts='2' THEN "Posted"
               WHEN a.batchsts='3' THEN "Error"
               WHEN a.batchsts='4' THEN "Deleted"
               ELSE "Open, Posted, Error, Deleted"
          END batchsts,
         case when a.readytpost='0' then "Open"
              when a.readytpost='1' then "ReadyToPost"
              else "Open & ReadyToPost"
         end batchrtp,
         case when b.doctype=4 then "IN"
	    when b.doctype=6 then "DN"
	    when b.doctype=5 then "CN"
	    when b.doctype=7 then "IT"
         end doctype,
		b.docentry,
		b.docnum,
		b.dscription,
		b.docdate,
		b.custmrcode,f.custmrname,
		b.taxcode taxgroup,
		b.docduedate duedate,
                     b.orderno ordernumber,
		b.termcode,
		b.yop,
		b.mop,
		b.taxcode taxauthoruty,
		b.doctotalamt netdistamt,
                     b.doctotalamt,b.discvalueamt,b.taxtotalamt ,
                     (ifnull(b.doctotalamt,0)-ifnull(b.discvalueamt,0)+ifnull(b.taxtotalamt,0))saldo,
                     b.discdate,b.discprcnt,
                     c.distcode,ifnull(g.dscription,'') distcodedesc,
		c.detailamt,
		c.taxamt,
		c.accrevcode,ifnull(d.accfmttd,'') glaccount,
		c.accrevdscription,
		case when b.doctype='4' then ifnull(SUM(c.detailamt+c.taxamt),0) else 0  end ttlinv,
		case when b.doctype='5' then ifnull(SUM(c.detailamt+c.taxamt),0) else 0  end ttldn,
		case when b.doctype='6' then ifnull(SUM(c.detailamt+c.taxamt),0) else 0  end ttlcn,
		case when b.doctype='7' then ifnull(SUM(c.detailamt+c.taxamt),0) else 0  end ttlint,
          CASE WHEN $P{entried}='1' AND UCASE($P{imported})='I' AND UCASE($P{transfer})='T' THEN "Entered"
               WHEN UCASE($P{entried})='E' AND $P{imported}='2' AND UCASE($P{transfer})='T' THEN "Imported"
               WHEN UCASE($P{entried})='E' AND UCASE($P{imported})='I' AND $P{transfer}='3' THEN "Generated"
               WHEN $P{entried}='1' AND UCASE($P{imported})='2' AND UCASE($P{transfer})='T' THEN "Entered,Import"
               WHEN $P{entried}='1' AND Ucase($P{imported})='I' AND UCASE($P{transfer})='3' THEN "Entered,Generated"
               WHEN UCASE($P{entried})='E' AND UCASE($P{imported})='2' AND $P{transfer}='3' THEN "Import,Generated"
               ELSE "Entered,Imported, Genereted"
          END ctktype
from ar_invbatchlist a
	inner join ar_invh b
		on a.batchno=b.batchno
	inner join ar_invd c
		on b.batchno=c.batchno
		and b.docentry=c.docentry
	left join gl_account d
		on  d.acccode=c.accrevcode
		and d.cmpnyid=a.cmpnyid
	left join ar_optional e
		on e.cmpnyid=a.cmpnyid
          	left join ar_customer f
		on f.custmrcode=b.custmrcode
          	left join ar_distcode g
		on g.distcode=c.distcode
where (a.batchno BETWEEN $P{batchnofr} and $P{batchnoto})
		and (left(a.batchdate,10) BETWEEN $P{batchdatefro} and $P{batchdatetoo})
		and (a.batchtype =$P{entried} or a.batchtype=$P{imported} or a.batchtype=$P{transfer})
		and (a.batchsts=$P{status1} or a.batchsts=$P{status2} or a.batchsts=$P{status3} or a.batchsts=$P{status4})
		and (a.readytpost=0 or a.readytpost=1)
		and (a.swprinted=$P{swprint}  or a.swprinted='1' or a.swprinted='0')
group by a.batchno,b.docentry,c.linenum
;]]>
	</queryString>
